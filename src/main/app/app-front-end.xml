    <http:listener-config name="HTTP_Listener_Configuration" host="0.0.0.0" port="8083" doc:name="HTTP Listener Configuration" />
    <apikit:config name="Router" raml="mule-coap-test-front-end.raml" consoleEnabled="true" consolePath="console" keepRamlBaseUri="false" doc:name="Router" />
    <flow name="app-front-end">
        <http:listener config-ref="HTTP_Listener_Configuration" path="/*" doc:name="HTTP" />
        <apikit:router config-ref="Router" doc:name="APIkit Router" />
