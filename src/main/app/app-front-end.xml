        <http:listener config-ref="HTTP_Listener_Configuration" path="/*" doc:name="HTTP" />
        <apikit:router config-ref="Router" doc:name="API Router" />
        <exception-strategy ref="ExceptionMapping" doc:name="Reference Exception Strategy" />
    </flow>
